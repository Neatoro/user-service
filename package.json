{
  "name": "user-service",
  "version": "1.0.0",
  "description": "A service to manage users via JWT tokens",
  "main": "dist/index.js",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "src/(.*)": [
        "<rootDir>/src/$1"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neatoro/user-service.git"
  },
  "author": "Moritz Schramm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Neatoro/user-service/issues"
  },
  "homepage": "https://github.com/Neatoro/user-service#readme",
  "scripts": {
    "start": "node .",
    "test:unit": "jest",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "build": "tsc"
  },
  "dependencies": {
    "@nestjs/common": "7.5.5",
    "@nestjs/config": "0.6.1",
    "@nestjs/core": "7.5.5",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.5.5",
    "@nestjs/typeorm": "7.1.5",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "helmet": "4.2.0",
    "mysql": "2.18.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "typeorm": "0.2.29"
  },
  "devDependencies": {
    "@types/jest": "26.0.16",
    "@types/node": "14.14.10",
    "babel-jest": "26.6.3",
    "jest": "26.6.3",
    "ts-jest": "26.4.4",
    "typescript": "4.1.2"
  }
}
